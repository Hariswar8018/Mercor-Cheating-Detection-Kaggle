   "source": [
    "import pandas as pd\n",
    "\n",
    "train = pd.read_csv(\"/kaggle/input/mercor-cheating-detection/train.csv\")\n",
    "\n",
    "features = [c for c in train.columns if c.startswith(\"feature_\")]\n",
    "\n",
    "labeled = train[train.is_cheating.notna()]\n",
    "\n",
    "X = labeled[features]\n",
    "y = labeled[\"is_cheating\"]"
   ]
  },
