    "hc_clean = train[train.high_conf_clean == 1].copy()\n",
    "hc_clean[\"is_cheating\"] = 0\n",
    "\n",
    "combined = pd.concat([labeled, hc_clean], ignore_index=True)\n",
    "\n",
    "weights = [1.0 if pd.isna(hc) else 0.25 for hc in combined.high_conf_clean]\n",
    "\n",
    "X_all = combined[features]\n",
    "y_all = combined[\"is_cheating\"]\n",
    "\n",
    "final_base_model = lgb.LGBMClassifier(\n",
    "    n_estimators=800,\n",
    "    learning_rate=0.03,\n",
    "    num_leaves=64,\n",
    "    class_weight=weights_cal,\n",
    "    random_state=42\n",
    ")\n",
    "\n",
    "final_base_model.fit(X_all, y_all, sample_weight=weights)"
